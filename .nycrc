{
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "statements": 95,
    "branches": 85,
    "functions": 100,
    "reporter": ["text-summary", "html", "json-summary"],
    "include": ["src/**/*.ts"],
    "exclude": ["src/main.ts", "src/**/*.test.ts", "src/cli.ts", "**/*.d.ts", "build/**", "test/**"],
    "extension": [".ts"],
    "require": ["ts-node/register"],
    "source-map": true,
    "instrument": true,
    "produce-source-map": true
}
